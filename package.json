{
  "name": "@appsweet-co/npm-carbon",
  "version": "1.0.12",
  "description": "Copy npm packages and all their versions from one registry to another",
  "main": "bin/index.js",
  "bin": {
    "npm-carbon": "bin/index.js"
  },
  "files": [
    "bin/**/*.js",
    "bin/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --pretty",
    "changelog": "npx auto-changelog",
    "deploy": "npm run publish:npm && npm run publish:github",
    "publish:github": "npm publish --registry='https://npm.pkg.github.com/'",
    "publish:npm": "npm publish --registry='https://registry.npmjs.org/'",
    "readme": "npx @appnest/readme generate",
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc --pretty --watch"
  },
  "keywords": [],
  "author": "Appsweet.co <https://appsweet.co/>",
  "license": "MIT",
  "dependencies": {
    "@dperuo/logger": "gist:fa069f4e73e795a8815cd7cf2ce0c4fd",
    "fibrous": "0.4.0",
    "npm-registry-client": "8.6.0",
    "typescript": "4.6.4",
    "yargs": "17.5.0"
  },
  "devDependencies": {
    "@types/node": "17.0.33",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0"
  }
}
